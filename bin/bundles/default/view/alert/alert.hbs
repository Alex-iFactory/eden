{{# error }}
    <div class="alert alert-danger" role="alert">
        <span class="fa fa-alert" aria-hidden="true"></span>
        <span class="sr-only">Error:</span>
        {{ . }}
    </div>
{{/ error }}

{{# success }}
    <div class="alert alert-success" role="alert">
        <span class="fa fa-alert" aria-hidden="true"></span>
        <span class="sr-only">Success:</span>
        {{ . }}
    </div>
{{/ success }}